{
    "rules": {
        "indent": [ 2, 2, {"SwitchCase": 1} ],
        "quotes": 0,
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 2, "always" ],
        "no-console": 0,
        "valid-jsdoc": 2
    },
    globals: {
        "gulp": true
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "modules": true,
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ]
}
